{
  "name": "dom-parse",
  "version": "3.1.0",
  "description": "HTML/XML to DOM parser for browsers and Node.js",
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/node.cjs",
  "module": "./dist/node.js",
  "browser": "./dist/browser.js",
  "exports": {
    ".": {
      "browser": {
        "import": "./dist/browser.js",
        "require": "./dist/browser.cjs"
      },
      "import": "./dist/node.js",
      "require": "./dist/node.cjs"
    },
    "./browser": {
      "import": "./dist/browser.js",
      "require": "./dist/browser.cjs"
    }
  },
  "scripts": {
    "build": "vite build",
    "coverage": "vitest run --coverage",
    "lint": "eslint .",
    "format": "prettier --write .",
    "prepare": "npm run build && husky",
    "preversion": "npm run lint && npm run test",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jacobbuck/dom-parse.git"
  },
  "keywords": [
    "document",
    "dom",
    "html",
    "mathml",
    "parser",
    "svg",
    "xhtml",
    "xml"
  ],
  "author": "Jacob Buck <buck.jacob@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jacobbuck/dom-parse/issues"
  },
  "homepage": "https://github.com/jacobbuck/dom-parse",
  "dependencies": {
    "happy-dom": "*"
  },
  "devDependencies": {
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vitest": "^0.3.22",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "vite": "^5.1.4",
    "vitest": "^1.3.1"
  }
}
