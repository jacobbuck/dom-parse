{
  "name": "dom-parse",
  "version": "2.0.3",
  "description": "Isomorphic library to safely parse an HTML string into DOM tree.",
  "main": "lib/server.cjs.js",
  "module": "lib/server.esm.js",
  "browser": {
    "./lib/server.cjs.js": "./lib/browser.cjs.js",
    "./lib/server.esm.js": "./lib/browser.esm.js"
  },
  "files": [
    "lib"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
